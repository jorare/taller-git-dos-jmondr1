public class Secuencia_1_al_100 
{

    public static void main(String[] args) 
    {
        int num;
        System.out.println("Secuencia de numeros");
        System.out.println("Del 0 al 100");
        System.out.print("Serie: ");

        for (num=1;num<=100;num++)
        {
            System.out.print(num+",");
        }
    }
}